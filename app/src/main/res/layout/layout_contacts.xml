<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">



    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imgPro"
        android:layout_width="181dp"
        android:layout_height="205dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/a" />

    <TextView
        android:id="@+id/tvname"
        android:layout_width="173dp"
        android:layout_height="51dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="52dp"
        android:text="Jemin Shrestha"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/imgPro"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvphone"
        android:layout_width="171dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:text="986051267"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/imgPro"
        app:layout_constraintTop_toBottomOf="@+id/tvname" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView2"
        android:layout_width="187dp"
        android:layout_height="203dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgPro"
        app:srcCompat="@drawable/b" />

    <TextView
        android:id="@+id/tvName2"
        android:layout_width="173dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:textSize="24sp"
        android:text="Aniket"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/tvphone" />

    <TextView
        android:id="@+id/tvPhone2"
        android:layout_width="169dp"
        android:layout_height="61dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:textSize="24sp"
        android:text="777777777"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/tvName2" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView3"
        android:layout_width="193dp"
        android:layout_height="218dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/c" />

    <TextView
        android:id="@+id/tvName3"
        android:layout_width="165dp"
        android:layout_height="62dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/tvPhone2" />

    <TextView
        android:id="@+id/tvphone3"
        android:layout_width="161dp"
        android:layout_height="58dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/tvName3" />
</androidx.constraintlayout.widget.ConstraintLayout>